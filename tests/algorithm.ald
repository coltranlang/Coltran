# Permutations
class Permutaion
    def permute(self, nums_list)
        let result = []
        
        if len(nums_list) == 1:
            return [nums_list[:]]
        end
        in range(len(nums_list)) as i:
            let n = nums_list.pop(0)
            let perms = self.permute(nums_list)
            in perms as perm:
                perm.append(n)
            end
            result.extend(perms)
            nums_list.append(n)
        end
        return result
    end
end

