let count = 0
for i = 0 to 10:
    for j = 0 to 10:
        for k = 0 to 10:
            if i != j and i != k and j != k:
                count += 1
            end
        end
    end
end
println(fm"Count = %{count}") # 720

let number = 0
let sum = 0

while number <= 50:
    sum += number
    number += 1
end

println(fm"Sum = %{sum}") # Sum = 1275
println(fm"Number = %{number}") # Number = 51

while true:
    println("Hello World")
    break
end